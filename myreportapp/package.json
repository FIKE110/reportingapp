{
  "name": "com.report.myreportapp",
  "displayName": "Report",
  "version": "1.0.0",
  "description": "An app where user can report incidents and events",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "web:devlive": "dir && browser-sync start --server 'www' --files 'www'"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^13.0.0",
    "cordova-browser": "^7.0.0",
    "cordova-plugin-camera": "^7.0.0",
    "cordova-plugin-geolocation": "^5.0.0",
    "cordova-plugin-x-toast": "^2.7.3"
  },
  "cordova": {
    "platforms": [
      "browser",
      "android"
    ],
    "plugins": {
      "cordova-plugin-camera": {
        "ANDROIDX_CORE_VERSION": "1.8.0"
      },
      "cordova-plugin-x-toast": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      }
    }
  },
  "dependencies": {
    "browser-sync": "^3.0.2",
    "framework7": "^8.3.3",
    "minimatch": "^10.0.1"
  }
